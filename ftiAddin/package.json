    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.8.1",
    "mocha": "^10.4.0",
    "moment-timezone": "^0.5.45",
    "puppeteer": "^19.8.3",
    "regenerator-runtime": "^0.14.1",
    "semver-regex": "^4.0.5",
    "start-server-and-test": "^2.0.3",
    "style-loader": "^3.3.4",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.10",
