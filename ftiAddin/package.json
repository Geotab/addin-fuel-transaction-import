  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "no-alert": 2
    },
    "globals": {
      "geotab": true,
      "dialogPolyfill": true,
      "GeotabApi": true,
      "rison": true
    }
  },
  "mocha": {
    "timeout": 10000,
    "reporter": "nyan"
  },
  "dependencies": {
    "luxon": "^3.4.4"
  }
